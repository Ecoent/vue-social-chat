  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.2.1",
    "@vue/compiler-sfc": "^3.0.5",
    "standard-version": "^9.3.0",
    "stylus": "^0.54.8",
    "vite": "^2.1.5",
    "vue-router": "^4.0.5"
  }
}
